{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    // "lib": [ "es2016" ],
    // "target": "ES2016",
    "strict": false,
    "allowJs": true,
    "noImplicitAny": false,
    "types": ["vite/client"],
    "paths": {
        "@/*": ["resources/js/*"]
    },
    "baseUrl": ".",
  },
  "include": [
    "resources/**/*.ts",
    "resources/**/*.d.ts",
    "resources/**/*.vue"
  ]
}
